{
  "name": "goblin-store",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html --open -p 3000",
    "build": "parcel build index.html --no-cache",
    "serve": "cd dist && npx serve",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "dev-server": "cd server && npm run dev",
    "dev-server:build": "cd server && npm i && cd ../",
    "start:dev": "concurrently --kill-others -n \"SERVER,APP\" \"npm:dev-server\" \"npm start\"",
    "start:dev-storybook": "concurrently --kill-others -n \"STORYBOOK, APP\" \"npm:storybook\" \"npm start\"",
    "postinstall": "npm run dev-server:build",
    "lint": "tsc --noEmit && eslint --ext .js,.ts,.tsx .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "components": "^0.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.5.1"
  }
}
